 "font": "Default",
 "html": "<body>\n    {% for row in doc.material_produce_item %}\n        {% if row.type == \"FG\" %}\n\n            {% set a %}{% endset %}\n            {% if row.item_code %}\n                {% set a=frappe.get_doc(\"Item\",row.item_code)%}\n            {% endif %}\n\n            {% set b %}{% endset %}\n            {% if row.item_code %}\n                {% set pl=frappe.db.get_single_value(\"Selling Settings\",\"selling_price_list\")%}\n                {% if pl %}\n                    {% set b=frappe.get_doc(\"Item Price\",{'item_code':row.item_code,'price_list':pl})%}\n                {% endif %}\n            {% endif %}\n\n            {% set c %}{% endset %}\n            {% if doc.work_order %}\n                {% set se = frappe.db.get_value(\"Stock Entry\" , { 'stock_entry_type':'Manufacture', 'work_order':doc.work_order, 'material_produce':doc.name} , 'name') %}\n                {% if se %}\n                    {% set c=frappe.db.get_value(\"Stock Entry\",se,'posting_date')%}\n                {% endif %}\n            {% endif %}\n            \n            {% set g %} {% endset %}\n            {% if doc.quality_inspection %}\n                {% set g = frappe.get_doc(\"Quality Inspection\",doc.quality_inspection) %}\n            {% endif %}\n\n            {% set e %} {% endset %}\n            {% set f %} {% endset %}\n            {% if doc.company %}\n                {% set e = frappe.get_doc(\"Dynamic Link\",{'link_doctype':'Company','link_name':doc.company}) %}\n                {% if e.parent %}\n                    {% set f = frappe.get_doc(\"Address\",e.parent) %}\n                {% endif %}\n            {% endif %}\n\n            <div style=\"border: 1px solid #000000;\">\n                <b style=\"font-size:22px;\"> <i>{{row.item_name}}</i></b><br>\n                <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                    <div class=\"col-xs-6\">{{ _(\"Net Weight \") }}</div>\n                    <div class=\"col-xs-6\">:&nbsp;{{ a.get_formatted(\"weight_per_unit\") }}&nbsp;{{ row.uom }}</div>\n                </div>\n                <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                    <div class=\"col-xs-6\">{{ _(\"MRP(incl all taxes) \") }}</div>\n                    <div class=\"col-xs-6\">:&nbsp;{{ b.get_formatted(\"price_list_rate\") }}</div>\n                </div>\n                <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                    <div class=\"col-xs-6\">{{ _(\"Packed On \") }}</div>\n                    <div class=\"col-xs-6\">:&nbsp;{{ c.strftime('%d-%m-%Y') }}</div>\n                </div>\n                <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                    <div class=\"col-xs-6\">{{ _(\"Batch No \") }}</div>\n                    <div class=\"col-xs-6\">:&nbsp;{% if g.batch_no %}{{ g.batch_no}} {% endif %} </div>\n                </div>\n\n                <div>\n                    <b style=\"font-size:22px;\">{% if a %}BEST BEFORE {{a.shelf_life_in_days}} DAYS FROM PACKAGING {% endif %}</b><br>\n                    <p style=\"font-size:22px;\">\n                        <i> {% if a.country_of_origin %}\n                                Country Of Origin {{a.country_of_origin}} \n                            {% endif %}\n                        </i>\n                    </p>\n                </div>\n\n                <div class=\"row\">\n                    <div class=\"col-xs-12\">\n                        <div class=\"col-xs-7\">\n                            <b style=\"font-size:21px;\">  Processed an Packed By: </b><br>\n                            <p style=\"font-size:18px;\"><b>{{doc.target_warehouse_display}}</p>\n                        </div>\n                        <div class=\"col-xs-5\">\n                            <span style=\"max-width:120%;max-height:50%;float:left;\">\n                                {% if g.batch_no %}\n                                <img width=\"150\" height=\"150\"\n                                    src=\"https://barcode.tec-it.com/barcode.ashx?data={{g.batch_no}}&code=QRCode\"\n                                    class=\"center\">\n                                <br>\n                                <p style=\"text-align:justify;\">{{g.batch_no}}</p><br>\n                                {% endif %}\n                            </span>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-12\">\n                        <div class=\"col-xs-7\">\n                            <b style=\"font-size:21px;\">  Marketed by: </b><br>\n                            <p style=\"font-size:18px;\"><b><i>{{doc.company}}</i></b></p>\n                            <p style=\"font-size:18px;\">\n                                {% if f.address_line1 %} \n                                    {{f.address_line1}}, \n                                {% endif %}<br>\n                                {% if f.address_line2 %} \n                                    {{f.address_line2}}, \n                                {% endif %}<br>\n                                {% if f.city %}\n                                    {{f.city}}\n                                {% endif %}\n                                {% if f.pincode %}\n                                    -{{f.pincode}},\n                                {% endif %}<br>\n                                {% if f.state %}\n                                    {{f.state}}\n                                {% endif %}\n                                {% if f.country %}\n                                    ,{{f.country}}\n                                {% endif %}.\n                            </p>\n                                    \n                            {% for i in f.lic%}\n                                <div class=\"row\">\n                                    <div style=\"text-align: left;\">\n                                        <div style=\"padding-left:7%\"><img style=\"max-width:25%;max-height:25px\" src=\"{{i.image}}\">\n                                        </div>\n                                    </div>\n                                </div>\n                                <p style=\"font-size:18px;\">Lic No :{{i.lic_no}}</p>\n                            {% endfor %}\n                            {% for bar in a.barcodes %}\n                            <img width=\"400\" height=\"100\" src=\"https://barcode.tec-it.com/barcode.ashx?data={{bar.barcode}}\">\n                            {% endfor %}\n                        </div>\n                    </div>\n                </div>\n            </div>    \n        {% endif %}\n    {% endfor %}\n</body>",
