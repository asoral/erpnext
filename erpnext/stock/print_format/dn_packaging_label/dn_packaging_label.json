 "html": "<html style=\"padding-left:5%;padding-right:5%;\">\n\n    <body>\n        {%for row in doc.items%}\n            {% set a %}{% endset %}\n            {% if row.item_code %}\n                {% set a=frappe.get_doc(\"Item\",row.item_code)%}\n            {% endif %}\n\n            {% set shipping_address %} {% endset %}\n            {% if doc.shipping_address_name %}\n                {% set shipping_address = frappe.get_doc(\"Address\",doc.shipping_address_name) %}\n            {% endif %}\n\n            {% set company_address_display %} {% endset %}\n            {% if doc.company_address %}\n                {% set company_address_display = frappe.get_doc(\"Address\",doc.company_address) %}\n            {% endif %}\n            <div style=\"border: 1px solid #000000;\">\n                <div class=\"row\" style=\"padding:3px;\">\n                    <div class=\"col-xs-12\">\n                        {{letter_head}}\n                    </div>\n                </div>\n                <div class=\"row\" style=\"padding:18px;\">\n                    <div> \n                        <b style=\"font-size:20px;\">\n                            {% if doc.select_print_heading%} \n                                {{doc.select_print_heading}} \n                            {%else%}\n                                {{doc.doctype}} \n                            {% endif %}\n                        </b>\n                        <br>\n                        {{doc.name}}\n                    </div>\n                </div>\n                <hr style=\"margin: 0px;margin-bottom: 2px;\">\n                <div style=\"padding-left:5%;padding-top:6%;\">\n                    <b style=\"font-size:22px;\"> <i>{{row.item_name}}</i></b>\n                    <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                        {% if row.total_weight %}\n                            <div class=\"col-xs-6\">{{ _(\"Net Weight \") }}</div>\n                            <div class=\"col-xs-6\">:&nbsp;{{ row.total_weight }}&nbsp;{{ row.uom }}</div>\n                        {% endif%}\n                    </div>\n\n                    <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                        {% if row.net_amount %}\n                        <div class=\"col-xs-6\">{{ _(\"MRP(incl. of all taxes) \") }}</div>\n                        <div class=\"col-xs-6\">:&nbsp;{{ row.get_formatted(\"net_amount\") }}</div>\n                        {% endif %}\n                    </div>\n\n                    <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                        {% if row.posting_date %}\n                        <div class=\"col-xs-6\">{{ _(\"Packed On \") }}</div>\n                        <div class=\"col-xs-6\">:&nbsp;{{ doc.get_formatted(\"posting_date\") }}</div>\n                        {% endif %}\n                    </div>\n\n                    <div style=\"font-size:19px;\" class=\"col-xs-12\">\n                        {% if  row.batch_no %}\n                        <div class=\"col-xs-6\"> {{ _(\"Batch No. \") }} </div>\n                        <div class=\"col-xs-6\">:&nbsp;{{ row.batch_no }}</div>\n                        {% endif %}\n                    </div>\n\n                    <div>\n                        <b style=\"font-size:20px;\">{% if a %}BEST BEFORE {{a.shelf_life_in_days}} DAYS FROM PACKAGING{% endif %} </b><br>\n\n                        <p style=\"font-size:20px;\"><i> {% if a.country_of_origin %}Country of Origin {% endif %}{% if\n                                a.country_of_origin %}{{a.country_of_origin}} {% endif %}\n                            </i></p><br>\n                    </div>\n                    <div class=\"row\">\n                        <div class=\"col-xs-12\">\n                            <div class=\"col-xs-7\">\n                                <b style=\"font-size:21px;\">Processed and Packed By </b><br>\n                                <p style=\"font-size:18px;\"><b><i>{{doc.customer}}</i></b></p><br>\n                                <p style=\"font-size:18px;\"> {% if doc.shipping_address %} {{doc.shipping_address }} {% endif %}</p>\n                            </div>\n                            <div class=\"col-xs-5\">\n                                <span style=\"max-width:120%;max-height:50%;float: right;\">\n                                    {% if row.batch_no %}\n                                    <img width=\"200\" height=\"200\" src=\"https://barcode.tec-it.com/barcode.ashx?data={{row.batch_no}}&code=QRCode\" class=\"center\">\n                                    <br>\n                                    <p style=\"text-align:justify;\">{{row.batch_no}}</p>\n                                    {% endif %}\n                    \n                                </span>\n                            </div>\n                        </div>\n                    </div>\n\n                    {% for i in shipping_address.lic%}\n                        <div class=\"row\">\n                            <div class=\"col-xs-4\" style=\"text-align: center;\">\n                                <div style=\"padding-left:7%\"><img style=\"max-width:19%;max-height:21px\" src=\"{{i.image}}\">\n                                </div>\n                            </div>\n                        </div>\n                        <p style=\"font-size:18px;\">Lic No :{{i.lic_no}} </p>\n                    {% endfor %}\n\n                    \n\n                </div>\n                <div>\n                    <div style=\"padding-left:5%;margin-bottom:50px;\">\n\n                        <b style=\"font-size:21px;\"> Shipping Address: </b><br>\n                        <p style=\"font-size:18px;\"><b><i>{{doc.company}}</i></b></p><br>\n                        <p style=\"font-size:18px;\">  {% if doc.company_address_display %} {{doc.company_address_display}} {%endif %}</p>\n\n                        {% for i in company_address_display.lic%}\n                        <div class=\"row\">\n                            <div class=\"col-xs-4\" style=\"text-align: center;\">\n                                <div style=\"padding-left:7%\"><img style=\"max-width:19%;max-height:21px\" src=\"{{i.image}}\">\n                                </div>\n                            </div>\n                        </div>\n\n                        <p style=\"font-size:18px;\">Lic No :{{i.lic_no}}</p>\n                        {% endfor %}\n\n                        <img width=\"400\" height=\"100\" src=\"https://barcode.tec-it.com/barcode.ashx?data={{row.barcode}}\">\n\n                    </div>\n                </div>\n                <div>{{footer}}</div>\n            </div>\n            <div class=\"page-break\"></div>\n        {%endfor%}\n    </body>\n</html>",
