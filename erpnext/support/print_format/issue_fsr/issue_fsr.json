rial %}\n                    {{ item.defective_part_serial }}\n                    /\n                  \n                {% elif item.defective_part_serial_hp %}\n                    {{ item.defective_part_serial_hp }}  \n                    /\n                {% endif %}\n                {% if item.defective_part_ct_no %}\n                        {{ item.defective_part_ct_no }}\n                {% endif %}  \n            </td>\n            <td class = \"td_blue\">{% if item.event %}\n                {{ item.event }}\n                {% endif %}\n            </td>\n            <td class = \"td_blue\"> \n                {% if item.replaced_part_description %}\n                    {{ item.replaced_part_description }}\n                {% endif %}\n            </td>\n            <td class = \"td_blue\">\n                {% if item.replaced_part %}\n                    {{ item.replaced_part }}\n                {% elif item.replaced_part_hp %}    \n                    {{ item.replaced_part_hp }}\n                {% endif %}\n            </td>\n            <td class = \"td_blue\">\n                {% if item.replaced_part_serial %}\n                    {{ item.replaced_part_serial }}\n                    /\n                {% elif item.replaced_part_serial_hp %}\n                    {{ item.replaced_part_serial_hp }}  \n                    /\n                {% endif %}\n                 {% if item.replaced_part_ct_no %}\n                        {{ item.replaced_part_ct_no }}\n                    {% endif %} \n            </td>\n           \n        </tr>\n        {%- endfor -%}\n        \n        \n        \n        \n</table>\n   <table class =\"ttrtd \">\n        <tr class =\"ttrtd \">\n            <td style=\"width:20%\" ><b>{{ _(\"Resolution \") }}</b></td>\n            <td style=\"width:80%\" >{% if doc.resolution_details %}\n                    {{ doc.resolution_details }}\n                {% endif %} \n            </td>\n        </tr>\n    </table>   \n    <table 