 "html": "{% set address %}{% endset %}\n{% if doc.customer_address %}\n    {%- set address=frappe.get_doc(\"Address\",doc.customer_address) -%}\n{% endif %}\n\n<!DOCTYPE HTML>\n{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n<div class=\"page\">\n<div class=\"text-center\"> {% if doc.select_print_heading %}\n                    {{ doc.select_print_heading }}\n                    {% endif %}\n</div>\n<table  class =\"ttrtd \" style = \" margin-top: 2px\">\n    <tr  class =\"ttrtd \">\n        <td style=\"text-align: center; font-size: 14px;\">\n            <b>FIELD SERVICE REPORT</b>\n        </td>\n    </tr>\n</table>\n<table class =\"ttrtd \">\n    <tr style=\"width:100%;\">\n        <td style=\"width:10%;\" ><b>{{ _(\"Ticket #\") }}</b></td>\n        <td style=\"width:20%;\">:\n        {% if doc.name %}\n        {{ doc.name }}\n        {% endif %}\n        </td>\n        \n        <td style=\"width:5%;\" ><b>{{ _(\"SUB\") }}</b></td>\n        <td style=\"width:35%;\">:\n        {% if doc.subject %}\n        {{ doc.subject }}\n        {% endif %}\n        </td>\n        \n        <td style=\"width:6%;\" ><b>{{ _(\"FSR #\") }}</b></td>\n        <td style=\"width:14%;\">:\n        {% if doc.fsr %}\n        {{ doc.fsr }}\n        {% endif %}\n        </td>\n    </tr>\n</table>\n\n<table  class =\"ttrtd \" style = \" margin-top: 2px\">\n    <tr  class =\"ttrtd \">\n        <td style=\"text-align: center; font-size: 10px;\">\n            <b>CUSTOMER INFORMATION</b>\n        </td>\n    </tr>\n</table>\n\n <table class =\"ttrtd \">\n        <tr class =\"ttrtd \">\n            <td class =\"ttrtd \" style= \"width: 50%\">\n                <div class= \"row\">\n                <div class=\"col-xs-10\">\n                        <b>{{ _(\"Customer Name\") }}</b>&nbsp; &nbsp; &nbsp; &nbsp; \n                    <!--</div>-->\n                    <!--<div class=\"col-xs-7\">-->\n                       : &nbsp;  {% if doc.customer %}\n                        <span style=\"font-size: 10px\">    {{ doc.customer }} </span>\n                            {% endif %} &nbsp; \n                        \n                    </div>\n                </div> \n                    <div class= \"row\">\n                        <div class=\"col-xs-10\">\n                            <b>{{ _(\"Telephone\") }}</b>&nbsp;  &nbsp; &nbsp; &nbsp;  &nbsp; &nbsp; &nbsp;  &nbsp; &nbsp;  \n                        <!--</div>-->\n                        <!--<div class=\"col-xs-8\">-->\n                             : &nbsp; {% if doc.telephone %}\n                         <span style=\"font-size: 10px\">   {{ doc.telephone }} </span>\n                            {% endif %} &nbsp; \n                        </div>\n                    </div> \n                   \n                   <div class= \"row\">\n                        <div class=\"col-xs-10\">\n                            <b>{{ _(\"Mobile\") }}</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \n                        <!--</div>-->\n                        <!--<div class=\"col-xs-8\">-->\n                             : &nbsp; {% if doc.mobile %}\n                         <span style=\"font-size: 10px\">   {{ doc.mobile }} </span>\n                            {% endif %}&nbsp; \n                        </div>\n                    \n                    </div> \n                    <div class= \"row\">\n                        <div class=\"col-xs-10\">\n                            <b>{{ _(\"Contact Person \") }}</b>&nbsp; &nbsp; &nbsp; &nbsp;\n                        <!--</div>-->\n                        <!--<div class=\"col-xs-8\">-->\n                             : &nbsp; {% if doc.contact_person %}\n                        <span style=\"font-size: 10px\" >  {{ doc.contact_person }}</span>\n                            {% endif %} &nbsp; \n                        </div>\n                    </div>\n            </td>\n            \n            <td style= \"width: 50%